{
  "compilerOptions": {
    "jsx": "react",
    "strict": true,
    "target": "ESNext",
    "declaration": true,
    "skipLibCheck": true,
    "alwaysStrict": true,
    "stripInternal": true,
    "esModuleInterop": true,
    "emitDeclarationOnly": true,
    "forceConsistentCasingInFileNames": true,
    "removeComments": true,
    "moduleResolution": "node"
  }
}